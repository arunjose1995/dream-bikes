{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": ["prettier", "airbnb-base"], 
    "plugins": ["prettier"],
    "overrides": [],
    "parserOptions": {
      "ecmaVersion": "latest"
    },
    "rules": {
      "prettier/prettier": "error",
      "quotes": ["single", { "aviodEscope": true }]
    }
}
